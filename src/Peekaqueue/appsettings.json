{
  "Monitoring": {
    "IntervalInSeconds": 15,
    "SqsConnectionRetryCount": 3,
    "SqsConnectionRetryBackoffMultiplier": 1,
    "Queues": [
      {
        "Name": "queue-name",
        "ConsumerCluster": "",
        "ConsumerService": ""
      }
    ],
    "MetricsEndpointPort": 5000,
    "MetricsEndpointPath": "metrics/",
    "CloudWatchNamespace": "Custom"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ColoredConsole",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Gateway.Api.Security.ApiKeyAuthenticationHandler": "Warning",
        "Couchbase": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "Peekaqueue"
    }
  }
}